@import url('https://fonts.googleapis.com/css2?family=Quicksand:wght@400&display=swap'); 

html, body, [class*="css"] {
    font-family: 'Quicksand', sans-serif; 
    font-size: 11px;
    font-weight: 500;
    color: #091747;
}

html, h1, h2, h3, h4 [class*="css"] {
    font-family: 'Quicksand', sans-serif; 
    font-size: 13px;
    font-weight: 500;
    color: #091747;
}